{
  "name": "questify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently \"npm run socket:dev\" \"npm run dev\"",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "pwa:build": "next build && next export",
    "socket:dev": "tsx server/socket-server.ts",
    "socket:build": "tsc server/socket-server.ts --outDir dist",
    "socket:start": "node dist/socket-server.js",
    "deploy:build": "npm run build && npm run socket:build",
    "deploy:start": "concurrently \"npm run start\" \"npm run socket:start\"",
    "deploy:prod": "npm run deploy:build && npm run deploy:start"
  },
